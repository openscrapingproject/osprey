{
    "name": "collection_name",
    "base_url": "https://www.cnn.com",
    "initial_urls": [
        "/2020/06/27/us/woodrow-wilson-princeton-university-trnd/index.html",
        "/2020/06/24/politics/christopher-wray-fbi-protests-foreign-influence/"
    ],
    "requestor": {
        "plugin": "builtin",
        "config": {
            "timeout": {
                "secs": 3,
                "nanos": 0
            },
            "headers": {
                "User-Agent": "openscraping 1.0"
            },
            "browser": [
                "Firefox",
                "random"
            ],
            "template": "default"
        }
    },
    "pages": {
        "<page_set_ID>": {
            "matcher": {
                "plugin": "regex",
                "config": {
                    "url": "/product/*/",
                    "headers": {
                        "Content-Type": "text/html"
                    }
                }
            },
            "extractor": {
                "plugin": "extractorID",
                "config": {
                    "extra": "config"
                },
                "definition": {
                    "scraping": "definition",
                    "can include": "hardcoded values",
                    "or use of": "XPATH, css or similar"
                }
            }
        }
    },
    "data": {
        "plugin": "output",
        "config": {
            "serialize": "json",
            "out": "stdout"
        }
    }
}